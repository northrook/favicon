{
  "name": "northrook/favicon",
  "description": "Latte integration for Symfony.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Favicon\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.2",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "northrook/core": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/types": "dev-main",
    "symfony/serializer": "@stable",
    "intervention/image": "@stable",
    "meyfa/php-svg": "@stable"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/deprecation-contracts": "@stable",
    "symfony/var-dumper": "@stable",
    "symfony/error-handler": "@stable"
  }
}